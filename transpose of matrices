

class transpose_of_matrices {
    public static void main(String[] args) {
        int [][]array={
            {1,2,3},
            {5,6,7},
            {9,10,11},
        };
        int rows=array.length;
        int cols=array[0].length;
        for(int i=0;i<cols;i++){
            for(int j=0;j<rows;j++){
                System.out.println(array[j][i] + " "); 
            }
            System.out.println();
        }
    }
}
